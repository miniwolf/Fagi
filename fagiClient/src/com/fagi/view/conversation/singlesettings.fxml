<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane styleClass="message" xmlns="http://javafx.com/javafx/8.0.60"
            xmlns:fx="http://javafx.com/fxml/1" stylesheets="@../../style/conversation.css">
    <top>
        <VBox>
            <HBox alignment="CENTER_LEFT" styleClass="message-title">
                <Pane>
                    <Pane layoutY="12.0" styleClass="picture"/>
                </Pane>
                <VBox styleClass="middle-holder">
                    <Label fx:id="name1" styleClass="mu" VBox.vgrow="ALWAYS"/>
                    <Label fx:id="date1" styleClass="last-seen" VBox.vgrow="NEVER"/>
                </VBox>
                <HBox styleClass="icon">
                    <Button styleClass="minimize"/>
                    <Button styleClass="popout"/>
                    <Button styleClass="close"/>
                </HBox>
            </HBox>
        </VBox>
    </top>
    <bottom>
        <HBox spacing="5.0" styleClass="RU">
            <VBox styleClass="ewgz9b" HBox.hgrow="ALWAYS">
                <Button cancelButton="true" maxWidth="1.7976931348623157E308"
                        mnemonicParsing="false"
                        text="Button" visible="false"/>
            </VBox>
            <VBox styleClass="ewgz9b" HBox.hgrow="ALWAYS">
                <Button defaultButton="true" maxWidth="1.7976931348623157E308" text="OK"
                        styleClass="zbg, cmx6we"/>
            </VBox>
        </HBox>
    </bottom>
    <center>
        <BorderPane styleClass="settingsmenu" BorderPane.alignment="CENTER">
            <top>
                <VBox styleClass="togglelist" BorderPane.alignment="TOP_LEFT">
                    <HBox alignment="CENTER_LEFT" styleClass="toggle-holder">
                        <Pane styleClass="button-wrapper">
                            <HBox styleClass="item">
                                <Pane styleClass="button-wrapper">
                                    <Button layoutY="3.0" mnemonicParsing="false" styleClass="check-box, untoggled"/>
                                </Pane>
                                <Label styleClass="toggle-message" text="Label"/>
                            </HBox>
                        </Pane>
                    </HBox>
                    <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="14.0"
                          styleClass="toggle-holder">
                        <Pane styleClass="button-wrapper">
                            <HBox styleClass="item">
                                <Pane styleClass="button-wrapper">
                                    <Button mnemonicParsing="false" styleClass="check-box, toggled"/>
                                </Pane>
                                <Label styleClass="toggle-message" text="Label"/>
                            </HBox>
                        </Pane>
                    </HBox>
                </VBox>
            </top>
            <center>
                <VBox BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets top="11.0"/>
                    </BorderPane.margin>
                    <HBox alignment="CENTER_LEFT" styleClass="vr, XX">
                        <Button mnemonicParsing="false" styleClass="zbg, archive, bu">
                            <HBox.margin>
                                <Insets right="12.0"/>
                            </HBox.margin>
                        </Button>
                        <Label text="Archive Conversation"/>
                    </HBox>
                    <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" styleClass="vr">
                        <Button mnemonicParsing="false" styleClass="zbg, delete, bu">
                            <HBox.margin>
                                <Insets right="12.0"/>
                            </HBox.margin>
                        </Button>
                        <Label text="Delete Conversation"/>
                    </HBox>
                    <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="110.0" styleClass="vr">
                        <Button mnemonicParsing="false" styleClass="zbg, block, bu">
                            <HBox.margin>
                                <Insets right="12.0"/>
                            </HBox.margin>
                        </Button>
                        <Label text="Block Marcus"/>
                    </HBox>
                </VBox>
            </center>
        </BorderPane>
    </center>
</BorderPane>
