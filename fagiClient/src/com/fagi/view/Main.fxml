<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<Pane fx:id="body" onMouseDragged="#mouseDragged" onMousePressed="#mousePressed"
      styleClass="maincontent" stylesheets="@../style/main.css, @../style/searchlist.css"
      xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
    <Pane layoutX="32.0" layoutY="14.0" styleClass="menubar">
        <Pane layoutX="6.0" layoutY="57.0" styleClass="shape">
            <Pane styleClass="chosen, message-button"/>
        </Pane>
        <Pane layoutX="6.0" layoutY="14.0" styleClass="shape">
            <Pane styleClass="contact-button, button-shape"/>
        </Pane>
    </Pane>
    <VBox layoutX="80.0" layoutY="80.0" styleClass="list">
        <Pane styleClass="search">
            <Pane layoutX="24.0" layoutY="16.0" styleClass="search-icon"/>
            <TextField fx:id="searchBox" layoutX="55.0" prefHeight="42.0" prefWidth="379.0"
                       promptText="New conversation" styleClass="search-area"/>
        </Pane>
        <ScrollPane fx:id="listContent" styleClass="list-content"/>
    </VBox>
    <Button layoutX="960.0" layoutY="-6.0" mnemonicParsing="false" onMousePressed="#logoutRequest"
            styleClass="rc-button-red, rc-button">
        <graphic>
            <Label styleClass="text" text="X"/>
        </graphic>
    </Button>
</Pane>
